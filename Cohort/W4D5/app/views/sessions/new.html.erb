<h2>User Log In Page</h2>
<%= render 'layouts/errors' %>

</div>
<form class="" action="<%= session_url %>" method="post">
  <%= auth_token %>
  <input type="hidden" name="_method" value="POST">
  <label for="user_email">Email:</label>
  <input type="text" id="user_email" name="user[email]" value="">
  <br>
  <label for="user_password">Password:</label>
  <input type="text" name="user[password]" value="">
  <br>
  <input type="submit" value="Log me in, Scotty!">
</form>
